{
  "name": "photo",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "all:install": "npm i --workspaces",
    "aws:clean": "aws s3 rm s3://peng-data-0005 --recursive && aws s3 rm s3://peng-0005-images --recursive && sam delete --stack-name peng-0005",
    "aws:package": "sam package --region eu-central-1 --s3-bucket peng-data-0005 --output-template-file output-template.yaml",
    "aws:deploy": "sam deploy --stack-name peng-0005 --template-file output-template.yaml",
    "aws:seed": "aws s3 cp ../../media s3://peng-0005-images/ --recursive",
    "aws:reset": "npm run aws:clean && npm run aws:package && npm run aws:deploy && npm run aws:seed"
  },
