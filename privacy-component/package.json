{
  "name": "privacy-component",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "aws:init": "aws s3 mb s3://peng-runtime",
    "aws:clean": "aws s3 rm s3://peng-runtime --recursive && aws s3 rm s3://peng-data --recursive && sam delete --stack-name peng",
    "aws:package": "sam package --region eu-central-1 --s3-bucket peng-runtime --output-template-file output-template.yaml",
    "aws:deploy": "sam deploy --stack-name peng --template-file output-template.yaml",
    "aws:seed": "aws s3 cp ../media s3://peng-data/ --recursive",
    "aws:reset": "npm run aws:clean && npm run aws:package && npm run aws:deploy && npm run aws:seed"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
  },
  "workspaces": [
    "./*"
  ],
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.11.0"
  }
}